#include <stdio.h>

void PrintNumPattern(int x, int y) {
  
   if (x < 0) {
      printf("%d ", x);
   }

   else {
      printf("%d ", x);            
      PrintNumPattern(x - y, y);              
      printf("%d ", x);
   }
}

int main(void){
   int num1;
   int num2;
   
   scanf("%d", &num1);
   scanf("%d", &num2);
   PrintNumPattern(num1, num2);
}
