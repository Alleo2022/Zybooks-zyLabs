#include <stdio.h>

int main(void) {


   /* Type your code here. */
  int userNum;
  int x;
   
   scanf ("%d" , &userNum);
   scanf ("%d" , &x);
   
   userNum = userNum / x;
   
   printf ("%d" , userNum);
   
   userNum = userNum / x;
   printf (" %d" , userNum);
   
   userNum = userNum / x;
   printf (" %d\n" , userNum);

   return 0;
}
