#include <stdio.h>

int main(void) {

int num1;
int num2;
int num3;
int small;

scanf ("%d", &num1);
scanf ("%d", &num2);
scanf ("%d", &num3);

if (((num1 < num2) && (num1 < num3)) != 0) {
    small = num1;
}
else if (((num2 < num1) && (num2 < num3)) != 0) {
    small = num2;
}
else {
    small = num3;
}
   printf("%d\n",small);
   return 0;
}
