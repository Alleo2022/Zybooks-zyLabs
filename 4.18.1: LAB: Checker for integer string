#include <stdio.h>
#include <string.h>

int main(void) {

   char str[50];
   int x = 1;
   
   scanf("%s", str);
   
   for(int i = 0; str[i] != '\0'; i++){
      
      if(isdigit(str[i]) == 0)
         x = 0;
   } 
   
   if(x == 0)
      printf("No\n");
   else
      printf("Yes\n");

   return 0;
}
