#include <stdio.h>
#include <string.h>

int main(void) {

char password[50];
char i;
char a;
char m;
char B;
char s;

scanf("%s", password);

for (i=0; strlen(password)> i; ++i)
   {
    if (password [i] == 'i'){
    password [i] = '1';
    }
    
        if (password [i] == 'a'){
    password [i] = '@';
    }
    
        if (password [i] == 'm'){
    password [i] = 'M';
    }
    
        if (password [i] == 'B'){
    password [i] = '8';
    }
    
        if (password [i] == 's'){
    password [i] = '$';
    }
   }

printf("%s!\n", password);

   return 0;
}
