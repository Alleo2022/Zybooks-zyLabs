#include <stdio.h>
#include <string.h>

int main(void) {
   char userString[50];
   int userNum;

   scanf ("%s", &userString);
   scanf("%d", &userNum);

   while (strcmp(userString, "quit") != 0){
      printf ("Eating %d %s a day keeps you happy and healthy.\n" , userNum, userString);
      scanf ("%s", &userString);
      scanf("%d", &userNum);
}

   return 0;
}
